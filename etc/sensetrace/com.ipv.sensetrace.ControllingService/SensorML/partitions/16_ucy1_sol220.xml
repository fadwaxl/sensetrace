<?xml version="1.0"?>
<SensorML xmlns:swe="http://www.opengis.net/swe" xmlns:gml="http://www.opengis.net/gml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xlink="http://www.w3.org/1999/xlink"
	xsi:schemaLocation="http://www.opengis.net/sensorML
http://vast.uah.edu/schemas/sensorML/1.0.30/base/sensorML.xsd"
	version="1.0">
	<System id="urn:ucy_1:sol220">
		<!--============================ -->
		<!-- Station Discovery Metadata -->
		<!--============================ -->
		<description>
			<swe:Discussion>
				Solarmodules of type Sol 220. Description in the
				manual "Jahresenergieerträge
				unterschiedlicher
				Photovoltaik-Technologien bei verschiedenen klimatischen Bedingungen
				- Bastian Zinßer"
			</swe:Discussion>
		</description>
		<identification>
			<IdentifierList>
				<identifier name="longName">
					<Term qualifier="urn:ogc:def:identifier:longName">
						Sol 220
					</Term>
				</identifier>
				<identifier name="shortName">
					<Term qualifier="urn:ogc:def:identifier:shortName">
						Sol 220
					</Term>
				</identifier>
			</IdentifierList>
		</identification>
		<contact>
			<ResponsibleParty gml:id="ipv_contact">
				<organizationName>Universität Stuttgart - Institut für Photovoltaik
				</organizationName>
				<contactInfo>
					<address>
						<electronicMailAddress>hendrik.adler@ipv.uni-stuttgart.de
						</electronicMailAddress>
					</address>
				</contactInfo>
			</ResponsibleParty>
		</contact>
		<!--========================== -->
		<!-- Station Coordinate Frame -->
		<!--========================== -->

		<!--================= -->
		<!-- Station Outputs -->
		<!--================= -->
		<outputs>
			<OutputList>
				<output name="moduleMeasurements">

					<swe:DataGroup>

						<swe:component name="P_AC_actual">
							<swe:Quantity definition="P_AC#04_actual"
								uom="ogc:def:unit:kilowatthours" sensorid="155" creationdate="2006-05-31"
								decimalplace="0" type="avg-1s" lowerLimit="0" upperLimit="2100"
								mysqlid="155" />
						</swe:component>
						<swe:component name="P_DC_1Sec">
							<swe:Quantity definition="P_DC#04_1Sec" uom="ogc:def:unit:watt"
								sensorid="154" creationdate="2006-05-31" decimalplace="1" type="avg-1s"
								lowerLimit="-10" upperLimit="2100" mysqlid="154" />
						</swe:component>
						<swe:component name="E_DC">
							<swe:Quantity definition="E_DC#04"
								uom="ogc:def:unit:kilowatthours" sensorid="153" creationdate="2006-05-31"
								decimalplace="0" type="avg-1s" lowerLimit="0" upperLimit="10000"
								mysqlid="153" />
						</swe:component>
						<swe:component name="T_mod_1Sec">
							<swe:Quantity definition="T_mod#04_1Sec" uom="ogc:def:unit:celsius"
								sensorid="152" creationdate="2006-05-31" decimalplace="1" type="avg-1s"
								lowerLimit="-10" upperLimit="90" mysqlid="152" />
						</swe:component>
						<swe:component name="I_DC_1Sec">
							<swe:Quantity definition="I_DC#04_1Sec" uom="ogc:def:unit:ampere"
								sensorid="151" creationdate="2006-05-31" decimalplace="3" type="avg-1s"
								lowerLimit="-0.02" upperLimit="15.6" mysqlid="151" />
						</swe:component>

						<swe:component name="V_DC_1Sec">
							<swe:Quantity definition="V_DC#04_1Sec" uom="ogc:def:unit:voltage"
								sensorid="150" creationdate="2006-05-31" decimalplace="1" type="avg-1s"
								lowerLimit="-10" upperLimit="350" mysqlid="150" />
						</swe:component>
						<swe:component name="E_AC">
							<swe:Quantity definition="E_AC#04"
								uom="urn:ogc:def:unit:kilowatthours" sensorid="149"
								creationdate="2006-05-31" decimalplace="3" type="avg-1s"
								lowerLimit="0" upperLimit="10000" mysqlid="149" />
						</swe:component>


					</swe:DataGroup>
				</output>
			</OutputList>
		</outputs>
		<!--================================= -->
		<!-- System Communication Interfaces -->
		<!--================================= -->
		<interfaces>
			<InterfaceList>
				<connection>
					<Link>
						<source ref="192.168.2.102/media/m01/mem308/memory.mem" />
						<destination ref="ftp" />
					</Link>
						<Link2>
						<source ref="ucy04_SOL220" />
						<destination ref="folder" />
					</Link2>
				</connection>
			</InterfaceList>
		</interfaces>
	</System>
</SensorML>
