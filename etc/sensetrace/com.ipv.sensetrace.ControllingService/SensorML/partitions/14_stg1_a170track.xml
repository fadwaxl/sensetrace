<?xml version="1.0"?>
<SensorML xmlns:swe="http://www.opengis.net/swe" xmlns:gml="http://www.opengis.net/gml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xlink="http://www.w3.org/1999/xlink"
	xsi:schemaLocation="http://www.opengis.net/sensorML
http://vast.uah.edu/schemas/sensorML/1.0.30/base/sensorML.xsd"
	version="1.0">
	<System id="urn:stg_1:a170track">
		<!--============================ -->
		<!-- Station Discovery Metadata -->
		<!--============================ -->
		<description>
			<swe:Discussion>
				Solarmodules of type A-170 tracker. Description
				in the manual "Jahresenergieerträge
				unterschiedlicher
				Photovoltaik-Technologien bei verschiedenen klimatischen Bedingungen
				- Bastian Zinßer"
			</swe:Discussion>
		</description>
		<identification>
			<IdentifierList>
				<identifier name="longName">
					<Term qualifier="urn:ogc:def:identifier:longName">
						 A-170 tracker
					</Term>
				</identifier>
				<identifier name="shortName">
					<Term qualifier="urn:ogc:def:identifier:shortName">
						 A-170 tracker
					</Term>
				</identifier>
			</IdentifierList>
		</identification>
		<contact>
			<ResponsibleParty gml:id="ipv_contact">
				<organizationName>Universität Stuttgart - Institut für Photovoltaik
				</organizationName>
				<contactInfo>
					<address>
						<electronicMailAddress>hendrik.adler@ipv.uni-stuttgart.de
						</electronicMailAddress>
					</address>
				</contactInfo>
			</ResponsibleParty>
		</contact>
		<!--========================== -->
		<!-- Station Coordinate Frame -->
		<!--========================== -->

		<!--================= -->
		<!-- Station Outputs -->
		<!--================= -->
		<outputs>
			<OutputList>
				<output name="moduleMeasurements">

					<swe:DataGroup>
						<swe:component name="E_AC">
							<swe:Quantity definition="E_AC#16"
								uom="urn:ogc:def:unit:kilowatthours" sensorid="121" creationdate="2006-05-31"
								decimalplace="3" type="avg-1s" lowerLimit="0" upperLimit="10000"
								mysqlid="121" active="false" />
						</swe:component>

						<swe:component name="P_AC_actual">
							<swe:Quantity definition="P_AC#16_actual"
								uom="ogc:def:unit:kilowatthours" sensorid="120" creationdate="2006-05-31"
								decimalplace="0" type="avg-1s" lowerLimit="0" upperLimit="1400"
								mysqlid="120" active="false" />
						</swe:component>

						<swe:component name="E_DC">
							<swe:Quantity definition="E_DC#16"
								uom="ogc:def:unit:kilowatthours" sensorid="119" creationdate="2006-05-31"
								decimalplace="0" type="avg-1s" lowerLimit="0" upperLimit="10000"
								mysqlid="119" active="false"/>
						</swe:component>

						<swe:component name="P_DC_1Sec">
							<swe:Quantity definition="P_DC#16_1Sec" uom="ogc:def:unit:watt"
								sensorid="118" creationdate="2006-05-31" decimalplace="1" type="avg-1s"
								lowerLimit="-10" upperLimit="1400" mysqlid="118" active="false"/>
						</swe:component>
						<swe:component name="T_mod_1Sec">
							<swe:Quantity definition="T_mod#16_1Sec" uom="ogc:def:unit:celsius"
								sensorid="117" creationdate="2006-05-31" decimalplace="1" type="avg-1s"
								lowerLimit="-30" upperLimit="90" mysqlid="117" active="false"/>
						</swe:component>
						<swe:component name="I_DC_1Sec">
							<swe:Quantity definition="I_DC#16_1Sec" uom="ogc:def:unit:ampere"
								sensorid="116" creationdate="2006-05-31" decimalplace="3" type="avg-1s"
								lowerLimit="-0.02" upperLimit="7.8" mysqlid="116" active="false"/>
						</swe:component>

						<swe:component name="V_DC_1Sec">
							<swe:Quantity definition="V_DC#16_1Sec" uom="ogc:def:unit:voltage"
								sensorid="115" creationdate="2006-05-31" decimalplace="1" type="avg-1s"
								lowerLimit="-10" upperLimit="350" mysqlid="115" active="false"/>
						</swe:component>


					</swe:DataGroup>
				</output>
			</OutputList>
		</outputs>
		<!--================================= -->
		<!-- System Communication Interfaces -->
		<!--================================= -->
		<interfaces>
			<InterfaceList>
				<connection>
					<Link>
						<source ref="192.168.1.102/media/m01/mem293/memory.mem" />
						<destination ref="ftp" />
					</Link>
					<Link2>
						<source ref="ipe16_A170track" />
						<destination ref="folder" />
					</Link2>
				</connection>
			</InterfaceList>
		</interfaces>
	</System>
</SensorML>
